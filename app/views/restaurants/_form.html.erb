<%= form_for(@restaurant) do |f| %>

<div class="">
  <%= f.label :user_id   %> <br />
  <%= f.number_field :user_id     %>
</div>

  <div class="">
    <%= f.label :name    %> <br />
    <%= f.text_field :name      %>
  </div>

  <div class="">
    <%= f.label :address %> <br />
    <%= f.text_field :address   %>
  </div>

  <div class="">
    <%= f.label :city    %> <br />
    <%= f.text_field :city    %>
  </div>

  <div class="">
    <%= f.label :postal_code    %> <br />
    <%= f.text_field :postal_code    %>
  </div>

  <div class="">
    <%= f.label :time_open  %> <br />
    <%= f.number_field :time_open      %>
  </div>

  <div class="">
    <%= f.label :time_close %> <br />
    <%= f.number_field :time_close     %>
  </div>

  <div class="">
    <%= f.label :capacity   %> <br />
    <%= f.number_field :capacity      %>
  </div>

  <div class="">
    <%= f.label :cuisine_id    %> <br />
    <%= f.select :cuisine_id , Cuisine.all.collect { |c| [c.name, c.id]}   %>
  </div>

  <div class="">
    <%= f.label :price_range %><br />
    <%= f.label :price_range_1, "$" %>
    <%= f.radio_button :price_range, 1 %>
    <%= f.label :price_range_2, "$$"%>
    <%= f.radio_button :price_range, 2 %>
    <%= f.label :price_range_3, "$$$" %>
    <%= f.radio_button :price_range, 3 %>
    <%= f.label :price_range_4, "$$$$"%>
    <%= f.radio_button :price_range, 4 %>
    <%= f.label :price_range_1, "$$$$$" %>
    <%= f.radio_button :price_range, 5 %>
r
  </div>


  <div class="">
    <%= f.label :image    %> <br />
    <%= f.text_field :image      %>
  </div>

  <div class=" ">
    <%= f.submit    %>
  </div>

  <% end %>
